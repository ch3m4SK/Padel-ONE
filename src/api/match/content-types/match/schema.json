{
  "kind": "collectionType",
  "collectionName": "matches",
  "info": {
    "singularName": "match",
    "pluralName": "matches",
    "displayName": "Match"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "matchId": {
      "type": "uid"
    },
    "pista": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::pista.pista",
      "inversedBy": "matches"
    },
    "player1": {
      "type": "string",
      "required": true
    },
    "player2": {
      "type": "string",
      "required": true
    },
    "startTime": {
      "type": "datetime",
      "required": true
    },
    "duration": {
      "type": "integer",
      "required": true
    },
    "state": {
      "type": "enumeration",
      "default": "scheduled",
      "enum": [
        "scheduled",
        "started",
        "finished"
      ]
    },
    "sets": {
      "type": "json",
      "required": true
    },
    "games": {
      "type": "json",
      "required": true
    },
    "winner": {
      "type": "string"
    }
  }
}
